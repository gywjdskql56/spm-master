  "name": "bazaar",
  "version": "3.4.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "export": "next build && next export",
    "build:local": "cross-env NODE_OPTIONS=--max-old-space-size=4096 next build",
    "start": "next start",
    "size": "node --max_old_space_size=4096",
    "fix:prettier": "prettier --write . '!.next' '!.node_modules'",
    "lint": "next lint",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "prod": "next export"
  },
  "browser": {
    "fs": false,
    "path": false,
    "os": false
  },
  "lint-staged": {
    "*.+(js|jsx)": [
